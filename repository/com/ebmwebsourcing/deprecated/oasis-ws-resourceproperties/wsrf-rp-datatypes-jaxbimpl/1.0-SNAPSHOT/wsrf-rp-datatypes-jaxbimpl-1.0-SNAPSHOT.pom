			<artifactId>wsrf-bf-datatypes-jaxbimpl</artifactId>
			<groupId>com.ebmwebsourcing.deprecated.oasis-ws-basefaults</groupId>
			<version>1.0-SNAPSHOT</version>
		</dependency>
		<!-- Test dependencies -->
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
		</dependency>
		<dependency>
			<groupId>com.ebmwebsourcing.deprecated.oasis-ws-resourceproperties</groupId>
			<artifactId>wsrf-rp-datatypes-api</artifactId>
			<version>1.0-SNAPSHOT</version>
			<type>test-jar</type>
			<scope>test</scope>
		</dependency>
		<dependency>
			<artifactId>wsaddressing10-api</artifactId>
			<groupId>com.ebmwebsourcing.wsstar.wsaddressing</groupId>
			<version>2.0-SNAPSHOT</version>
			<type>test-jar</type>
			<scope>test</scope>
		</dependency>
		<dependency>
			<artifactId>wsaddressing10-impl</artifactId>
			<groupId>com.ebmwebsourcing.wsstar.wsaddressing</groupId>
			<version>2.0-SNAPSHOT</version>
			<type>test-jar</type>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>com.ebmwebsourcing.deprecated.oasis-ws-basefaults</groupId>
			<artifactId>wsrf-bf-datatypes-api</artifactId>
			<version>1.0-SNAPSHOT</version>
			<type>test-jar</type>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>com.ebmwebsourcing.deprecated.oasis-ws-basefaults</groupId>
			<artifactId>wsrf-bf-datatypes-jaxbimpl</artifactId>
			<version>1.0-SNAPSHOT</version>
			<type>test-jar</type>
			<scope>test</scope>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>org.jvnet.jaxb2.maven2</groupId>
				<artifactId>maven-jaxb2-plugin</artifactId>
				<configuration>
					<extension>true</extension>
					<schemaDirectory>
						${basedir}/src/main/resources/schemas
					</schemaDirectory>
					<schemaIncludes>
						<!-- WS-ResourceProperties -->
						<include>rp-2.xsd</include>
					</schemaIncludes>
					<bindingIncludes>
						<include>binding.xjb</include>
					</bindingIncludes>
				</configuration>
				<executions>
					<execution>
						<goals>
							<goal>generate</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
			<!-- fix "XmlAnyAttribute inheritence" Problem in Generated Jaxb classes -->
			<plugin>
				<artifactId>maven-antrun-plugin</artifactId>
				<version>1.6</version>
				<executions>
					<execution>
						<id>wsrf-bugfix</id>
						<phase>process-resources</phase>
						<goals>
							<goal>run</goal>
						</goals>
						<configuration>
							<target>
								<!-- echo message="Change otherAttributes attribute visibility from 
									private to protected" /> <ant antfile="${basedir}/build.xml"> <target name="change-attribute-visibility"/ -->
								<echo
									message="disable @XmlAnyAttribute annotation (inheritence from BaseFaultType)" />
								<ant antfile="${basedir}/build.xml">
									<target name="comment-annotation-and-codeline" />
								</ant>
							</target>
						</configuration>
					</execution>
				</executions>
			</plugin>
			
						<!-- FIXME: Add cxf catalog to avoid to duplicate class -->
			<plugin>
				<artifactId>maven-antrun-plugin</artifactId>
				<executions>
					<execution>
						<id>delete-duplicate-classes</id>
						<phase>process-resources</phase>
						<goals>
							<goal>run</goal>
						</goals>
						<configuration>
							<tasks>
								<echo message="delete duplicate classes" />
								<delete dir="./target/generated-sources/xjc/easybox/org/w3" />
								<delete dir="./target/classes/easybox/org/w3" />
								<delete
									dir="./target/generated-sources/xjc/easybox/org/oasis_open/docs/wsrf/bf_2" />
								<delete dir="./target/classes/easybox/org/oasis_open/docs/wsrf/bf_2" />
								
								<delete
									dir="./target/generated-sources/xjc/easybox/org/oasis_open/docs/wsn/t_1" />
								<delete dir="./target/classes/easybox/org/oasis_open/docs/wsn/t_1" />
							</tasks>
						</configuration>
					</execution>
				</executions>
			</plugin>
			
			<!-- TEMPORARY : disable Unit Test -->
			<!-- <plugin> <groupId>org.apache.maven.plugins</groupId> <artifactId>maven-surefire-plugin</artifactId> 
				<version>2.7.1</version> <configuration> <skipTests>true</skipTests> </configuration> 
				</plugin> -->
		</plugins>
	</build>
	<scm>
		<connection>scm:svn:https://svn.petalslink.org/svnroot/trunk/research/dev/experimental/deprecated/oasis-ws-resourceproperties/wsrf-rp-datatypes-jaxbimpl</connection>
		<developerConnection>${project.scm.connection}</developerConnection>
	</scm>
